{
  "name": "pwa-starter-kit",
  "version": "0.9.0",
  "contributors": [
    "The Polymer Authors"
  ],
  "license": "BSD-3-Clause",
  "repository": "Polymer/pwa-starter-kit",
  "scripts": {
    "start": "polymer serve",
    "build": "polymer build",
    "build:static": "polymer build",
    "serve": "prpl-server --root server/build",
    "serve:static": "polymer serve --port 5000 build/es5-bundled",
    "test": "npm run test:unit && npm run test:integration",
    "test:integration": "mocha test/integration --timeout=10000",
    "test:unit": "polymer test --module-resolution=node --npm",
    "test:sauce": "polymer test --module-resolution=node --npm -s 'windows 10/microsoftedge@17' -s 'macos 10.13/safari@11'",
    "test:regenerate_screenshots": "mocha test/integration/screenshots-baseline/regenerate.js --timeout=15000"
  },
  "dependencies": {
    "@appnest/lit-translate": "^1.1.16",
    "@polymer/app-layout": "^3.0.0",
    "@polymer/polymer": "^3.1.0",
    "@webcomponents/webcomponentsjs": "^2.2.4",
    "lit-element": "^2.0.1",
    "lit-translate": "^1.1.20",
    "moment": "^2.13.0",
    "pwa-helpers": "^0.9.0",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "webview-tile-header": "git+https://github.com/moduware/webview-tile-header.git#lit"
  },
  "devDependencies": {
    "@moduware/morph-button": "^2.0.0",
    "@moduware/morph-list-view": "github:moduware/morph-list-view",
    "@moduware/morph-list-view-item": "github:moduware/morph-list-view-item",
    "@moduware/morph-list-view-title": "github:moduware/morph-list-view-title",
    "@moduware/morph-pages": "git+https://github.com/moduware/morph-pages.git#lit-element",
    "@moduware/morph-swipeout": "github:moduware/morph-swipeout",
    "@moduware/morph-tabbar": "github:moduware/morph-tabbar",
    "@moduware/morph-tabbar-item": "github:moduware/morph-tabbar-item",
    "@moduware/morph-radio": "git+https://github.com/moduware/morph-radio.git#master",
    "@polymer/test-fixture": "^4.0.2",
    "axe-core": "^3.0.0",
    "chai": "^4.1.2",
    "del": "^3.0.0",
    "gulp": "^4.0.0",
    "gulp-rename": "^1.3.0",
    "gulp-replace": "^1.0.0",
    "material-design-lite": "^1.3.0",
    "mocha": "^5.2.0",
    "pixelmatch": "^4.0.2",
    "polymer-cli": "^1.9.11",
    "polyserve": "^0.27.0",
    "prpl-server": "^1.4.0",
    "puppeteer": "^1.5.0",
    "reset-css": "^5.0.1",
    "swiper": "^5.3.0",
    "wct-mocha": "^1.0.1"
  }
}
