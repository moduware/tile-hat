{
	"entrypoint": "index.html",
	"basePath": "./",
	"shell": "src/components/my-app.js",
	"sources": [
		"images/**/*"
	],
	"extraDependencies": [
		"manifest.json",
		"icon.svg",
		"node_modules/@webcomponents/webcomponentsjs/**",
		"node_modules/moment/moment.js",
    "fonts/**"
	],
	"builds": [
		{
			"name": "nexpaq.tile.hat.develop-pwa",
			"js": {
				"compile": "es5",
				"minify": false,
				"transformModulesToAmd": true
			},
			"css": {
				"minify": false
			},
			"html": {
				"minify": false
			},
			"bundle": true
		}
	],
	"moduleResolution": "node",
	"npm": true
}
